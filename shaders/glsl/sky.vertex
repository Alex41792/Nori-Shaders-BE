// __multiversion__

#include "vertexVersionSimple.h"
#include "uniformWorldConstants.h"

attribute vec4 POSITION;

varying float sd;

void main(){
    vec4 position = POSITION;
        position.y -= length(position.xz) * 0.15;
    gl_Position = WORLDVIEWPROJ * position;

    sd = length(POSITION.xz);
}
